# Alias

# Because typing cd is too musch work
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'

# Quick acces rc files
alias vimrc='vim ~/.vimrc'
alias zshrc='vim ~/.zshrc'

# For vim
alias v='vim'
alias vi='vim'

# Change to your last known directory
alias ocd='cd -'

alias startfastai='gcloud compute instances start my-fastai-instance'
alias sshfastai='gcloud compute ssh --zone=asia-southeast1-b jupyter@my-fastai-instance -- -L 8080:localhost:8080'
alias stopfastai='gcloud compute instances stop my-fastai-instance'
